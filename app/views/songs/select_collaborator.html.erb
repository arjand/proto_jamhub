<% provide :title, "Musicians" %>

<table class="table">
  <tr>
    <td><b>Jam Name</b></td>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.jam_name %></td>
      <td><%= button_to 'Select',
          {:controller => :songs, :action => :add_collaborator,
         :user_id => user.id, :id => @song.id},
         :method => :put, :class => 'btn btn-small btn-primary' %></td>
    </tr>
  <% end %>
</table>
